snippet func
	func ${1:new_func}(${2}) {
		${3}
	}

snippet main
	func main() {
		${1}
	}

snippet pkg
	package ${1}

snippet imp
	import "${1}"

snippet for
	for ${1} {
		${2}
	}

snippet fore
	for ${1}, ${2} := range ${3} {
		${4}
	}

snippet fori
	for i int = ${1}; i < ${2}; i++ {
		${3}
	}

snippet if
	if ${1} {
		${2}
	}

snippet types
	type ${1} struct {
		${2}
	}

snippet typei
	type ${1} interface {
		${2}
	}

snippet pf
	fmt.Printf(${1});

#
snippet p
	println(${1});

